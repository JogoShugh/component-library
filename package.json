{
  "private": true,
  "name": "@versionone/component-library",
  "description": "Collection of components and accompanying documentation site LifeCycle and Continuum.",
  "version": "0.0.0",
  "collaborators": [
    "Andrew Smith <andrew@andrew.codes>"
  ],
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "start": "docz dev",
    "verify": "concurrently 'yarn lint' 'yarn test'",
    "lint": "yarn lint:eslint",
    "build": "lerna run build",
    "prebuild:website": "yarn bootstrap",
    "build:website": "docz build",
    "test": "start-test start http-get://localhost:3000 test:cy:run",
    "test:ci": "start-server-and-test start http-get://localhost:3000 test:cy:run:ci",
    "test:cy:run": "cypress run",
    "test:cy:run:ci": "cypress run",
    "lint:eslint": "eslint --print-config .eslintrc.json | eslint-config-prettier-check && eslint .",
    "publish:patch": "lerna publish patch",
    "postpublish:patch": "yarn publish:docs",
    "prepublish:docs": "cross-env BASE_URL=component-library yarn build:website",
    "publish:docs": "gh-pages -d ./.docz/dist"
  },
  "engines": {
    "node": "^8.6.0 || ^10.0.0",
    "yarn": "^1.3.0"
  },
  "workspaces": [
    "build/*",
    "packages/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@versionone/babel-plugin-react-docgen": "^0.0.2",
    "@versionone/theme-definition-handler": "^0.0.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-react-fela-display-name": "^1.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.3",
    "docz": "^0.12.15",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-cypress": "^2.1.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "lerna": "^3.4.3",
    "prettier": "^1.15.3",
    "react": "^16.6.1",
    "react-docgen-deprecation-handler": "^1.0.1",
    "react-dom": "^16.6.1",
    "start-server-and-test": "^1.7.11",
    "typescript": "^3.1.6",
    "webpack": "^4.27.1"
  }
}
